FoamFile
{
    version     2.0;
    format      ascii;
    class       volTensorField;
    location    "0";
    object      K;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 0 0 0 0 0];

internalField   uniform (1e-03 0 0 0 1e-03 0 0 0 1e-03);

boundaryField
{
    inlet
    {
        type            zeroGradient;
    }

    outlet
    {
        type            zeroGradient;
    }

    upperWall
    {
        type            cyclic;
    }
    lowerWall
    {
        type            cyclic;
    }
    frontAndBack
    {
        type            empty;
    }
}


